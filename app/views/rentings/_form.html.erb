<%= form_for([@house, @renting]) do |f| %>

  <div class="form-group">
    <%= f.label :country%>
    <%= f.text_field :country, class: "form-control"  %>
  </div>
  <div class="form-group">
    <%= f.label :state %>
    <%= f.text_field :state, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :city %>
    <%= f.text_field :city, class: "form-control" %>
  </div>
  <div class="actions">
    <%= f.submit class: "btn btn-success" %>
  </div><br>
<% end %>
