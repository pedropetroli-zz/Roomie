<%= form_for([@house, @renting]) do |f| %>

  <div class="form-group">
    <%= f.label :arrival%>
    <%= f.text_field :arrival, class: "form-control"  %>
  </div>
  <div class="form-group">
    <%= f.label :departure %>
    <%= f.text_field :departure, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :insurance %>
    <%= f.text_field :insurance, class: "form-control" %>
  </div>
  <div class="actions">
    <%= f.submit class: "btn btn-success" %>
  </div><br>
<% end %>
