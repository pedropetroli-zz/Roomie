<h1>Listing house_searches</h1>

<table class="table table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th class="center">House</th>
      <th class="center">Arrival</th>
      <th class="center">Departure</th>
      <th class="center">Total amounth</th>
      <th class="center">Rental</th>
      <th class="center">Fee</th>
      <th class="center">Insurance</th>
      <th class="center" colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @house_searches.each do |s| %>
      <tr>
        <td><%= s.house_id %></td>
        <td><%= s.arrival %></td>
        <td><%= s.departure %></td>
        <td><%= s.total_amounth %></td>
        <td><%= s.rental %></td>
        <td><%= s.fee %></td>
        <td><%= s.insurance %></td>
        <td><%= link_to 'Show', s %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New House search', new_house_search_path %>
